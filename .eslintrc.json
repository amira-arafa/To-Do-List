{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "prettier",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "ecmaVersion": 11
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"]
    }
  }
  